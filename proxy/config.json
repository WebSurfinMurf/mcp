{
  "mcpProxy": {
    "addr": ":9090",
    "baseURL": "http://localhost:9090",
    "name": "Local MCP Proxy",
    "type": "streamable-http",
    "options": {
      "logEnabled": true,
      "panicIfInvalid": false
    }
  },
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem@0.6.2", "/workspace"],
      "env": {
        "NODE_NO_WARNINGS": "1"
      }
    },
    "postgres": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-postgres@0.6.2", "postgresql://admin:Pass123qp@postgres:5432/postgres"],
      "env": {
        "NODE_NO_WARNINGS": "1"
      }
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer@2025.5.12"],
      "env": {
        "NODE_NO_WARNINGS": "1"
      }
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory@2025.9.25"],
      "env": {
        "NODE_NO_WARNINGS": "1"
      }
    },
    "minio": {
      "command": "/wrappers/minio-wrapper.sh",
      "args": []
    },
    "n8n": {
      "command": "/wrappers/n8n-wrapper.sh",
      "args": []
    },
    "timescaledb": {
      "command": "/wrappers/timescaledb-wrapper.sh",
      "args": []
    },
    "ib": {
      "command": "/wrappers/ib-wrapper.sh",
      "args": []
    }
  }
}
